<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.techtown.GmanService.FirstToMain">
    <!-- 인터넷 접속 권한 -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 인터넷 접속이 되어있는지 안되있는지 식별하기 위한 권한 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!-- 인터넷 포그라운드 권한 -->
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" /> <!-- 읽어오기, 수정 권한(디비) -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <!-- 구글 애드몹 -->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/app_id" />

        <activity android:name="org.techtown.GmanService.FirstToMain.MaintoEnd.Settings.SmokeReSettings"></activity>
        <activity android:name="org.techtown.GmanService.FirstToMain.MaintoEnd.Special.List.RecyclerMain" />
        <activity android:name="org.techtown.GmanService.FirstToMain.MaintoEnd.Special.ReviseDiary" />
        <activity android:name="org.techtown.GmanService.FirstToMain.MaintoEnd.Special.WriteDiary" />
        <activity android:name="org.techtown.GmanService.FirstToMain.MaintoEnd.Special.ViewDiary" />
        <activity android:name="org.techtown.GmanService.FirstToMain.MaintoEnd.Notice.Notice"/>
        <!-- 프로필 사진 업로드 시 외부저장소에 대한 퍼미션 -->
        <uses-library
            android:name="sooyeol"
            android:required="false" />

        <activity
            android:name="org.techtown.GmanService.FirstToMain.homeMain.Loading_Dialog"
            tools:ignore="Instantiatable" />

        <service
            android:name="org.techtown.GmanService.FirstToMain.homeMain.MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name="org.techtown.GmanService.FirstToMain.findPW.AuthforPw" />
        <activity android:name="org.techtown.GmanService.FirstToMain.findPW.ChangePw" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.register.Register_Auth"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.start.Quest1"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.start.First_page_loading"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.homeMain.BottomNavi"
            android:label="@string/title_activity_bottom_navi"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.register.Register"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.start.Startbutton"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.findid.FindId"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.findPW.FindPw"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.findid.Id_pw_complete"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.techtown.GmanService.FirstToMain.login_fitstPage.Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>